{
  "expo": {
    "name": "Argus Security",
    "slug": "argus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.jpg",
    "splash": {
      "image": "./assets/UniversalAssets/Logo1024.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4F5F9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.argussecurityservices.mobileappargus"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/UniversalAssets/Logo256.png",
        "backgroundColor": "#F4F5F9"
      },
      "package": "com.argussecurityservices.mobileappargus"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
